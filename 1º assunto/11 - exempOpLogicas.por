programa
{
	funcao inicio()
	{
		escreva("Os resultados lógicos de:\n")
		escreva("1: (7 > 5) é ",7 > 5,"\n")
		logico valLog = 2 < 3
		escreva("2: (7 > 5) e (2 < 3) é ",(7 > 5) e valLog,"\n")
		
		escreva("3: (7 < 5) ou (nao(2 > 3)) é ",(7 < 5) ou (nao(2 > 3)),"\n")

		escreva("4: 2 == 1 + 1 é ",2 == 1 + 1,"\n")

		escreva("5: 3 != 1 é ",3 != 1,"\n")
	}
}

