programa
{
	funcao inicio()
	{
		real a,b,c
		
		escreva("Entre com A, B e C\n")
		leia(a,b,c)

		se((a >= b) e (a <= c))
  		{
  			escreva("A esta entre B e C\n")
  		}
		senao
  		{
  			escreva("A nao esta entre B e C\n")
  		}		
	}
}
