programa
{
	funcao inicio()
	{
		inteiro a,b,c
		
		escreva("Entre com A, B e C\n")
		leia(a,b,c)

		se(nao(c > 5))
		{
  			escreva ("a + b = ",a," + ",b," = ",a + b,"\n")
		}
		senao
		{
			escreva ("a - b = ",a," - ",b," = ",a - b,"\n")
		}
	}
}
